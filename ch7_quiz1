#include <iostream>
using namespace std;

class CPoint{
    private:
        int x, y;
    public:
        CPoint(int a=0, int b=0):x(a), y(b){}
        void Print();
        friend CPoint operator-(CPoint P1, CPoint P2);
};
void CPoint::Print(){cout << '(' << x << ',' << y << ')' << endl;}

CPoint operator-(CPoint P1){
    CPoint tmp;
    tmp.x=-1*this->x; tmp.y=-1*this->y;
}

int main(void){
    CPoint P1(2,2);
    CPoint P2=-P1;
    CPoint P3= -(-P1);

    P1.Print();
    P2.Print();
    P3.Print();
    return 0;
}


//사진 참조 0429

#include <iostream>
#include <vector>
using namespace std;

int main(){
    int i;
    vector <int> intV(5);
    int *pV = &intV[0];
    cout << "정수값 5개 입력: " ;
    for(i=0; i<5; i++){
        cin >> (*pV); pV++;
    }

    pV = &intV[0];
    cout << "정수값 5개 출력: ";
    for(i=0; i<5; i++){
        cout << (*pV) << "\t"; pV++;
    }
    cout << endl;
}

/*
일반 포인터 변수를 이용해서 결과값이 나오도록한 문제이다.
*/

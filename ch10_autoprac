#include <iostream>
#include <vector>
using namespace std;

int main(){
    vector<int> v;
    v.push_back(1);
    v.push_back(2);
    v.push_back(3);

    //vector<int>::iterator it;

    for(auto it=v.begin(); it!=v.end(); it++){
        int n = *it;
        n=n*2;
        *it =n;
    }

    for(auto it=v.begin(); it != v.end(); it++){
        cout << *it << ' ';
        cout << endl;
    }
}

//여기서는 auto 함수를 사용하는 예시를 보여준 것임. it를 별도로 선언하지 않게 되면, 이게 Int형 벡터일지, 다른 형태일지 모름
//그래서 에러가 발생하는 것인데, auto 를 활용하면, 가장 첫번째 벡터 값을 이용해서, 이게 int 형임을 알게 되고, 자동으로 이를 처리하게 함
